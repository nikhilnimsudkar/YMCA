<%@ include file="../../layouts/include_taglib.jsp"%>

<div id="tabstrip3" class="k-block"
	style="background-color: #FFFFFF; margin-top: 0px;">
	<span class="head" width="100%" style="margin-left: 3px;">CURRENT
		MEMBERSHIP DETAILS</span></br>

	<table cellpadding="0" cellspacing="0" width="97%" rules="none"
		style="margin-left: 7px;">
		<tr align="left">
			<th>ProductId</th>
			<td>${member.productId}</td>
		</tr>
		<tr align="left">
			<th>PricingRuleId</th>
			<td>${member.pricingRuleId}</td>
		<tr align="left">
			<th>MembershipNumber</th>
			<td>${member.membershipNumber}</td>
		</tr>
		<tr align="left">
			<th>ContactName</th>
			<td>${member.contactName}</td>
		</tr>
		<tr align="left">
			<th>ContactPartyId</th>
			<td>${member.contactPartyId}</td>
		</tr>
		<tr align="left">
			<th>CustomerPartyId</th>
			<td>${member.customerPartyId}</td>
		</tr>
		<tr align="left">
			<th>membershipPrice</th>
			<td>${member.membershipPrice}</td>
		</tr>
		<tr align="left">
			<th>MembershipType</th>
			<td>${member.membershipType}</td>
		</tr>
		<tr align="left">
			<th>MembershipDuration</th>
			<td>${member.membershipDuration}</td>
		</tr>
		<tr align="left">
			<th>MembershipBeginDate</th>
			<td><fmt:formatDate type="date" pattern="MM/dd/yyyy"
					value="${member.membershipBeginDate}" /></td>
		</tr>
		<tr align="left">
			<th>MembershipEndDate</th>
			<td><fmt:formatDate type="date" pattern="MM/dd/yyyy"
					value="${member.membershipEndDate}" /></td>
		</tr>

	</table>

</div>


