a.add-child-lnk span {
font-size: 20px;
font-weight: normal;
border-radius: 3px;
background: #fff;
border: 1px solid #009ddc;
padding: 0px 3px;
vertical-align: middle;
}


.add-child-lnk {
float: right;
font-size: 11px;
color: #009ddc;
cursor: pointer;
}

a.remove-child-lnk span {
font-size: 20px;
font-weight: normal;
border-radius: 3px;
background: #fff;
border: 1px solid #009ddc;
padding: 0px 6px 0px 6px;
vertical-align: middle;
}


.remove-child-lnk {
float: right;
font-size: 11px;
color: #009ddc;
cursor: pointer;
}

#productPriceTable tr td {
    padding-right: 0px !important;
}

.ttip {
    position: absolute;
    width: 355px;
    height: 120px;
    color: #fff;
    padding: 20px;
    -webkit-box-shadow: 1px 1px 1px 2px #CCC;
    -moz-box-shadow: 1px 1px 1px 2px #CCC;
    box-shadow: 1px 1px 1px 2px #CCC;
    border-radius: 8px 8px 8px 8px;
    -moz-border-radius: 8px 8px 8px 8px;
    -webkit-border-radius: 8px 8px 8px 8px;
    -o-border-radius: 8px 8px 8px 8px;    
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#F45000', endColorstr='#FF8000', GradientType=0);
    background-color:#fff;
    display: none
}
.contents {
    font-size: 15px;
    font-weight:bold
}
.note {
    font-size: 13px;
    text-align:center;
    display:block;
    width: 100%
}
#background{
    display: none;
    position: absolute;
    height: 100%;
    width: 100%;
    top: 0;
    left: 0;
    background: #000000;
    z-index: 1;
}
#large {
    display: none;
    position: absolute;
    background: #FFFFFF;
    padding: 0px;
    z-index: 10;
    min-height: 0px;
    min-width: 0px;
    color: #336699;
}

span.closeBtn{
	height : 50px !important;
	width : 50px !important;
}

table#selectFamilyMemberTable th, table#selectKidsInfoTable th{
	background-color: #fcaf17;
	color : white
}

table#selectFamilyMemberTable, table#selectKidsInfoTable {
	border: 2px outset grey
}

