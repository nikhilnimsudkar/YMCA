/* CSS Document */
body{
	width:80%;
	height:auto;
	min-height:400px;
	font-family:Verdana, Arial, Helvetica, sans-serif;
	font-size:13px;
	margin-left:10%;
}
#wrapper{
	width:100%;
}
#main{
	width:100%;
	height:auto;
	min-height:400px;
	font-family:Verdana, Arial, Helvetica, sans-serif;
	font-size:13px;
	margin-top:5px;
}

#header {
	height:150px;
	border:0px solid;
}

#content {
	height:auto;
	border:0px solid;
	overflow:auto;
	width:100%
}

#footer {
	height:80px;
	border:0px solid;
}

#member{
	height:auto;
	min-height:400px;
	float:left;
	width:31%;
	border:0px solid;
	padding-left:0px;
}

#profile{
	height:auto;
	min-height:400px;
	float:left;
	width:31%;
	border:0px solid;
	margin-left:77px;
	margin-right:0px;
	margin-top:20px;
}

#update{
	height:auto;
	min-height:400px;
	float:right;
	width:30%;
	border:0px solid;
	padding-right:0px;
	margin-top:20px;
}

#tabstrip, #tabstrip1, #tabstrip2 {
	width: 404px;
	margin: 20px auto;
}

.profilepic {
	display: inline-block;
	margin: 10px 0 10px 0px;
	width: 100px;
	height: 140px;	
	border:0px solid;
}

#team {
	width: 265px;
	padding: 5px 0 0 0;
	float: right;
	border:0px solid;
}

#tabstrip h2, #tabstrip1 h2, #tabstrip2 h2 {
	font-weight: lighter;
	font-size: 5em;
	padding: 0;
	margin: 0;
}

#tabstrip h2 span, #tabstrip1 h2 span, #tabstrip2 h2 span {
	background: none;
	padding-left: 5px;
	font-size: .5em;
	vertical-align: top;
}

#tabstrip p, #tabstrip1 p, #tabstrip2 p {
	margin: 0;
	padding: 0;
}

#tabstrip span.head, #tabstrip1 span.head, #tabstrip2 span.head {
	color:#006B6B;
}

#tabstrip span, #tabstrip1 span, #tabstrip2 span {
	font-weight:bold;
}

div.moreinfo{
	font-size: 14px;
	color:#F78F1E;
	font-weight: bold;
	padding-bottom:5px;
	padding-left:3px;
}
div.moreinfoenrolled{
	font-size: 14px;
	color:#14AE96;
	font-weight: bold;
	padding-bottom:5px;
	padding-left:3px;
}
span.viewmore{
	color:#999999;
	font-size: 12px;
	cursor: pointer
}

h2 span{
	font-weight:bold;
	color:#006B6B;
	font-size:16px;
}

#logo{
	float:left;
	margin-left:36px;
	width:15%;
	height:80px;
	background: url('../img/logo.png') transparent no-repeat;
	background-size: 100px 80px;
}

#right_menu{
	width:80%;
	float:right;
}

#page_name{
	text-align: right;
	margin-right: 60px;
	color: #F37418;
	font-weight:bold;
	font-size: 30px;
}
#top{
	height:100px;
	margin-top:15px;
}
br { clear:left }

.nav {
    width: 100%;
    background-color: #01A490;
}

.nav ul {
    margin: 0; padding: 0;
    float: left;
}

.nav ul li {
    display: inline;
}

.nav ul li a {
    float: left; text-decoration: none;
    color: white; 
    padding: 5px 24px;
    background-color:  #01A490;
    font-size: 15px;
    font-weight: bold;
}
 
.nav ul li a:visited {
    color: white;
}
 
.nav ul li a:hover, .nav ul li .current {
    color: white;
    background-color: #FCAF17;
}
#top_nav{
	
	height:40px;
	margin-right:62px;
	border:0px solid;
}
#top_nav ul {
    margin: 0; padding: 0;
    float: right;
}
#top_nav ul li {
    display: inline;
}
#top_nav ul li a {
    float: left; 
    text-decoration: none;
    color: #CBCBCB; 
    padding: 9px 10px;
    font-size: 11px;
    font-weight: bold;
}
#footer_main{
	height:30px;
}
#footer_main ul{
	margin: 0; padding: 0;
    float: left;
}
#footer_main ul li {
    display: inline;
}
#footer_main ul li a {
    float: left; 
    text-decoration: none;
    color: #CBCBCB; 
    padding: 9px 18px;
    font-size: 14px;
}

.copyright{
	font-size: 11px;
	 text-decoration: none;
    color: #CBCBCB; 
    margin: 9px 18px;
    float:left;
}
#tcErrorSpan{
	display:none;
	margin-left:8px;
	padding:9px;
}

#updateProfInfo{
	display:none;
	margin-left:8px;
	padding:9px;
}
#statusBlock{
	height:80px;
}