.nav {
    width: 100%;
    background-color: #01A490;
}

.nav ul {
    margin: 0; padding: 0;
    float: left;
    list-style:none;
	position:relative;
}

.nav ul li {
    position:relative;
	float:left;
	margin:0;
	padding:0;
	z-index:9999;
}

.nav ul li a {
    float: left; text-decoration: none;
    color: white; 
    padding: 5px 15px;
    background-color:  #01A490;
    font-size: 14px;
    font-weight: bold;
}
 
.nav ul li a:visited {
    color: white;
}
 
.nav ul li a:hover, .nav ul li .current {
    color: white;
    background-color: #FCAF17;
}

.nav ul ul{
	display:none;
	position:absolute;
	top:100%;
	left:0;
	background:#fff;
	padding:0
}

.nav ul li:hover > ul {
   display:block;
   height:auto; 
  
}

.nav ul li ul li{
    clear:both;
	border-style:none;
	width:220px
}
