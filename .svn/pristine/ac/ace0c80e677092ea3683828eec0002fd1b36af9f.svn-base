					#if (joinFee > 0) { #
					<p class="join_fee" id="join_fee_#= uniqueId #">
						<span>JOIN FEE:</span>
						<span class="table-price cartpricedisplay" data-bind="text: joinFeeTxt"><span>
					</p>
