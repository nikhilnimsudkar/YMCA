     $('#pmPrimary').on( "click", function() {        	
      	//$(".k-loading-mask").show();
  		//$(".k-loading-text").html("Please wait...");	
  		var paymentId = $('input[name=paymentInfoRadio]:checked').parent().parent().parent().parent().find('.pmMakePrimaryPaymentId').html();  		
