<script>
function validateUserName($email) {
	  var emailReg = /^([\w-\.]+@([\w-]+\.)+[\w-]{2,4})?$/;
	  if( !emailReg.test( $email ) ) {
	    return false;
	  } else {
	    return true;
	  }
}

$(document).ready(function(){
	$("input[type='checkbox']").uniform();
	$("input[type='radio']").uniform();
	$("#errorGenericDialogBox").dialog({
        autoOpen: false,
        modal:true,
        resizable: false,
        width:400,        
        buttons: {
    		Ok: function() {
      			$(this).dialog("close");
      			    			
    		} 
		}
    });
});
</script>
