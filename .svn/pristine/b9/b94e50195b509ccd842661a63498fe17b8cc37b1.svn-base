				  $("#tcSuccessSpan" ).html("");	
				  $("#tcErrorSpan").css("display", "block");		
				  $( "#tcErrorSpan" ).html("The Promo code you enetered is invalid");
				  return;
			  }
		  },
		  error: function( xhr,status,error ){
			  //alert("1" +status);
			  console.log(xhr);
			 
		  }
	});
	 
	 if (!found) {
		 $("#tcSuccessSpan").css("display", "none");		
		  $("#tcSuccessSpan" ).html("");	
		  $("#tcErrorSpan").css("display", "block");		
		  $( "#tcErrorSpan" ).html("The Promo code you enetered is invalid");
