function replaceAll(string, find, replace) {
	  return string.replace(new RegExp(escapeRegExp(find), 'g'), replace);
}

function getActivitiesyByCIdAndItemId(cId,itemId){
	var actParam = "";
	if (typeof(activityDataSource) != "undefined") {
		var data = activityDataSource.data();
		for (var i = 0; i < data.length; i++) {
			var actData = data[i] ;	
			if (cId == actData.cId && itemId == actData.parentItemId && (actData.priority != null || actData.priority != 'null')) {
				actParam = actParam  + actData.cId + "$" + actData.parentItemId +  "$" + actData.associatedItemId + "$"  + actData.priority + "#";
			}
		}
	}
	console.log(" actParam "+actParam);
	if (actParam == "") actParam = " ";
	return actParam;
}

