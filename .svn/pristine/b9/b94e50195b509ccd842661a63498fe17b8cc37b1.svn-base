		  type: "GET",
		  async: false,
		  url:"getPromodetails?promocode="+promocode,
		  dataType: "json",
		  success: function( data ) {
			  console.log(data);
			  if(data.length==1){
				  $.each(data, function(i,promo) {
					  if(promo.itemDetails.length>0){
						  
						  for (var c = 0; c < cartObj.contents.length; c ++) {
							  
								  $.each(promo.itemDetails, function(j,itemInfo) {
									  
