						  $("#tcErrorSpan1").css("display", "block");		
						  $( "#tcErrorSpan1" ).html("User Information already exists!");
						  $(".k-loading-mask").hide();
				  	  }
				  	  else{
				  		  $("#tcSuccessSpan1").css("display", "none");		
						  $("#tcSuccessSpan1" ).html("");	
						  $("#tcErrorSpan1").css("display", "block");		
						  $( "#tcErrorSpan1" ).html("There was some error. Please try again later");
						  $(".k-loading-mask").hide();
				  	  }
				  }else{					  
					  $("#tcSuccessSpan1").css("display", "none");		
					  $("#tcSuccessSpan1" ).html("");	
					  $("#tcErrorSpan1").css("display", "block");		
